{
  "$schema": "https://www.getbifrost.ai/schema",
  "vector_store": {
    "enabled": true,
    "type": "redis_cluster",
    "config": {
      "addrs": [
        "localhost:6371",
        "localhost:6372",
        "localhost:6373"
      ]
    }
  },
  "plugins": [
    {
      "enabled": true,
      "name": "semantic_cache",
      "config": {
        "cache_key": "bifrost-cckey",
        "cache_ttl_key": "bifrost-cc-ttl",        
        "prefix": "bifrost-cc-",
        "cache_by_model": true,
        "cache_by_provider": true
      }
    }
  ]
}